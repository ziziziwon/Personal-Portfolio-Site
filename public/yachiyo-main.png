<svg width="600" height="300" xmlns="http://www.w3.org/2000/svg">
  <!-- 배경 -->
  <rect width="600" height="300" fill="#87CEEB"/>
  
  <!-- 바다 -->
  <ellipse cx="300" cy="250" rx="400" ry="100" fill="#4682B4" opacity="0.5"/>
  
  <!-- 말풍선 -->
  <ellipse cx="250" cy="100" rx="120" ry="60" fill="#fff" stroke="#000" stroke-width="2"/>
  <polygon points="200,130 180,160 210,140" fill="#fff" stroke="#000" stroke-width="2"/>
  <text x="250" y="110" font-family="Malgun Gothic" font-size="32" fill="#000" text-anchor="middle">
    야치요의 방
  </text>
  
  <!-- 야치요 캐릭터 (햄스터 같은 귀여운 생물) -->
  <g transform="translate(400, 200)">
    <!-- 몸통 -->
    <ellipse cx="0" cy="0" rx="80" ry="60" fill="#fff" stroke="#000" stroke-width="2"/>
    
    <!-- 왼쪽 귀 -->
    <ellipse cx="-30" cy="-50" rx="20" ry="30" fill="#fff" stroke="#000" stroke-width="2"/>
    <ellipse cx="-30" cy="-50" rx="12" ry="20" fill="#ffb6c1"/>
    
    <!-- 오른쪽 귀 -->
    <ellipse cx="30" cy="-50" rx="20" ry="30" fill="#fff" stroke="#000" stroke-width="2"/>
    <ellipse cx="30" cy="-50" rx="12" ry="20" fill="#ffb6c1"/>
    
    <!-- 얼굴 -->
    <circle cx="-20" cy="-10" r="4" fill="#000"/>
    <circle cx="20" cy="-10" r="4" fill="#000"/>
    <path d="M -15 5 Q 0 12 15 5" stroke="#000" stroke-width="2" fill="none"/>
    
    <!-- 수염 점 -->
    <circle cx="-40" cy="0" r="2" fill="#000"/>
    <circle cx="-40" cy="10" r="2" fill="#000"/>
    <circle cx="40" cy="0" r="2" fill="#000"/>
    <circle cx="40" cy="10" r="2" fill="#000"/>
    
    <!-- 발 -->
    <ellipse cx="-30" cy="50" rx="20" ry="15" fill="#fff" stroke="#000" stroke-width="2"/>
    <ellipse cx="30" cy="50" rx="20" ry="15" fill="#fff" stroke="#000" stroke-width="2"/>
  </g>
  
  <!-- 바다 식물들 -->
  <g opacity="0.6">
    <path d="M 50 300 Q 60 250 50 200 Q 40 250 50 300" fill="#ff6b6b" stroke="#d63031" stroke-width="2"/>
    <path d="M 150 300 Q 160 260 150 220 Q 140 260 150 300" fill="#ffa502" stroke="#ff7f00" stroke-width="2"/>
    <path d="M 500 300 Q 510 250 500 200 Q 490 250 500 300" fill="#4ecdc4" stroke="#1abc9c" stroke-width="2"/>
    <path d="M 550 300 Q 560 270 550 230 Q 540 270 550 300" fill="#ff6348" stroke="#e84118" stroke-width="2"/>
  </g>
  
  <!-- 물방울 -->
  <circle cx="100" cy="100" r="5" fill="#fff" opacity="0.6"/>
  <circle cx="500" cy="80" r="6" fill="#fff" opacity="0.6"/>
  <circle cx="300" cy="60" r="4" fill="#fff" opacity="0.6"/>
</svg>
